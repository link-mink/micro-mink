{
  "mqtt": {
    "connections": [
      {
        "name": "mqtt_local",
        "address": "tcp://127.0.0.1:1883",
        "client_id": "mqtt_unit_tester",
        "username": "user",
        "password": "password",
        "keep_alive": 3600,
        "proc_thread_qsize": 256,
        "bin_upload_path": "/tmp/mink.bin",
        "subscriptions": [
          "mink/DEBUG_UUID/+/cmd",
          "mink.bin/#"
        ]
      }
    ]
  },
  "umlua": {
    "db": "test/test.db",
    "aggressive_gc": true,
    "conserve_memory": true,
    "envs": [
      {
        "name": "mqtt_rx",
        "auto_start": false,
        "interval": 0,
        "path": "test/mqtt_rx_test.lua",
        "events": [
          "mqtt:RX"
        ]
      },
      {
        "name": "TEST_MQTT_01",
        "auto_start": false,
        "interval": 0,
        "path": "test/mqtt_test_01.lua",
        "events": [
          "TEST_MQTT_01"
        ]
      },
      {
        "name": "TEST_MQTT_02",
        "auto_start": false,
        "interval": 0,
        "path": "test/mqtt_test_02.lua",
        "events": [
          "TEST_MQTT_02"
        ]
      },
      {
        "name": "TEST_MQTT_03",
        "auto_start": false,
        "interval": 0,
        "path": "test/mqtt_test_03.lua",
        "events": [
          "TEST_MQTT_03"
        ]
      },
      {
        "name": "TEST_MQTT_04",
        "auto_start": false,
        "interval": 0,
        "path": "test/mqtt_test_04.lua",
        "events": [
          "TEST_MQTT_04"
        ]
      }
    ]
  }
}
